@model IEnumerable<NK_EMPWeb.Models.EmployeeManagement.Emp_ClientMasterDetailsModels>

@{
    ViewBag.Title = "ClientDetails";
}

<!-- breadcrumb -->
<div class="breadcrumb-header justify-content-between">
    <div class="my-auto">
        <div class="d-flex">
            <h4 class="content-title mb-0 my-auto">Employee Mangment</h4><span class="text-muted mt-1 tx-13 ml-2 mb-0">/ Client View</span>
        </div>
    </div>
</div>
<!-- breadcrumb -->
<!-- row -->
<div class="row">
    <div class="col-xl-12 col-lg-12">


        <!--Entry Code start-->
        <div class="card custom-card">
            <div class="card-body">
                @foreach (var item in Model)
                {
                    <div class="card-body pt-0">
                        <div class="mg-b-50">
                            <div class="row row-sm mg-b-20">
                                <div class="col-lg-3">
                                    <span class="tx-bold">
                                        @Html.DisplayNameFor(model => model.ClientName)
                                    </span>
                                </div>
                                <div class="col-lg-3">
                                    <label for="">
                                        @item.ClientName
                                    </label>
                                </div>
                                <div class="col-lg-3">
                                    <span class="tx-bold">
                                        @Html.DisplayNameFor(model => model.ClientContact)
                                    </span>
                                </div>
                                <div class="col-lg-3">
                                    <label for="">
                                        @item.ClientContact
                                    </label>
                                </div>
                            </div>

                            <div class="row row-sm mg-b-20">
                                <div class="col-lg-3">
                                    <span class="tx-bold">
                                        @Html.DisplayNameFor(model => model.ClientAddress)
                                    </span>
                                </div>
                                <div class="col-lg-3">
                                    <label for="">
                                        @item.ClientAddress
                                    </label>
                                </div>
                                <div class="col-lg-3">
                                    <span class="tx-bold">
                                        @Html.DisplayNameFor(model => model.ClientCity)
                                    </span>
                                </div>
                                <div class="col-lg-3">
                                    <label for="">
                                        @item.ClientCity
                                    </label>
                                </div>
                            </div>

                            <div class="row row-sm mg-b-20">
                                <div class="col-lg-3">
                                    <span class="tx-bold">
                                        @Html.DisplayNameFor(model => model.ClientState)
                                    </span>
                                </div>
                                <div class="col-lg-3">
                                    <label for="">
                                        @item.ClientState
                                    </label>
                                </div>
                                <div class="col-lg-3">
                                    <span class="tx-bold">
                                        @Html.DisplayNameFor(model => model.ClientCountry)
                                    </span>
                                </div>
                                <div class="col-lg-3">
                                    <label for="">
                                        @item.ClientCountry
                                    </label>
                                </div>
                            </div>

                            <div class="row row-sm mg-b-20">
                                <div class="col-lg-3">
                                    <span class="tx-bold">
                                        @Html.DisplayNameFor(model => model.ClientDesignation)
                                    </span>
                                </div>
                                <div class="col-lg-3">
                                    <label for="">
                                        @item.ClientDesignation
                                    </label>
                                </div>
                                <div class="col-lg-3">
                                    <span class="tx-bold">
                                        @Html.DisplayNameFor(model => model.ClientMailID)
                                    </span>
                                </div>
                                <div class="col-lg-3">
                                    <label for="">
                                        @item.ClientMailID
                                    </label>
                                </div>
                            </div>

                            <div class="row row-sm mg-b-20">
                                <div class="col-lg-3">
                                    <span class="tx-bold">
                                        @Html.DisplayNameFor(model => model.ClientGSTNO)
                                    </span>
                                </div>
                                <div class="col-lg-3">
                                    <label for="">
                                        @item.ClientGSTNO
                                    </label>
                                </div>

                                <div class="col-lg-3">
                                    <span class="tx-bold">
                                        @Html.DisplayNameFor(model => model.ClientPanCardNo)
                                    </span>
                                </div>
                                <div class="col-lg-3">
                                    <label for="">
                                        @item.ClientPanCardNo
                                    </label>
                                </div>
                            </div>
                            <div class="row row-sm mg-b-20">
                                <div class="col-lg-3">
                                    <span class="tx-bold">
                                        @Html.DisplayNameFor(model => model.ClientRemark)
                                    </span>
                                </div>
                                <div class="col-lg-3">
                                    <label for="">
                                        @item.ClientRemark
                                    </label>
                                </div>

                                <div class="col-lg-3">
                                    <span class="tx-bold">
                                    </span>
                                </div>
                                <div class="col-lg-3"></div>
                            </div>

                        </div>

                        <div class="row row-sm text-center justify-content-center">
                            <div class="col-lg-3">
                                <button type="button" onclick="back()" class="btn btn-light btn-block">Back</button>
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
        <!--Entry Code End-->

    </div>
</div>
<!-- row closed -->
@section scripts {
    <script src="~/Scripts/Angular/EmployeeManagement/Services/ClientMasterServices.js"></script>
}